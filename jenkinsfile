pipeline {
    agent any
    
    tools{
        maven 'myname'
    }

    stages {
        stage('compile') {
            steps {
                echo 'compile hello-world'
                sh 'mvn compile'
                
            }
        }
        stage('test') {
            steps {
                echo 'test hello-world'
                sh 'mvn test'
            }
        }
        stage('package') {
            steps {
                echo 'package hello-world in Env'
                sh 'mvn package'
            }
        }
        
    }
}
