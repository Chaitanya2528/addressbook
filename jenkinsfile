pipeline {
    agent any

    tools{
        tool name: 'mymaven', type: 'maven'
    }
    
    stages {
        stage('compile') {
            steps {
                echo 'compile the code'
                sh 'mvn compile'
            }
        }
        stage('test') {
            steps {
                echo 'test the code'
                sh 'mvn test'
            }
        }
        stage('package') {
            steps {
                echo 'package the code in Env'
                sh 'mvn package'
            }
        }
        
    }
}